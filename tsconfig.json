{
  "compilerOptions": {
    "resolveJsonModule": true,
    "jsx": "preserve",
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": false,
    "moduleResolution": "node",
    "strictBindCallApply": true,
    "declaration": false,
    "esModuleInterop": true,
    "lib": ["ESNext.Intl"],
    "types": ["node", "jest"],
    "sourceMap": true,
    "isolatedModules": true,
    "noEmit": true
  }
}
